{
  "name": "faraway",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "vite build && mv build/files build/assets/files",
    "build-storybook": "VITE_API_HOST=msw storybook build",
    "check-types": "tsc --noEmit",
    "dev": "vite",
    "dev:mocks": "VITE_API_HOST=msw vite",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "prepare": "husky install",
    "prettier": "prettier --check --ignore-path .gitignore \"**/*.+(html|json|mdx|yml)\"",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "stylelint": "stylelint -f verbose --ignore-path .gitignore \"**/*.+(css|scss)\"",
    "svgo": "svgo -f ./src/assets/svg --config=./svgo.config.cjs",
    "test": "vitest",
    "test-storybook": "test-storybook"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.1.1",
    "antd": "^5.10.2",
    "camelcase-keys": "^8.0.2",
    "classnames": "^2.3.2",
    "date-fns": "^2.30.0",
    "decamelize-keys": "^2.0.1",
    "qs": "^6.11.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.45.0",
    "react-nl2br": "^1.0.4",
    "react-router-dom": "^6.14.1",
    "react-use": "^17.4.0",
    "swr": "^2.1.5",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.2.0",
    "@storybook/addon-interactions": "^7.2.0",
    "@storybook/addon-links": "^7.2.0",
    "@storybook/blocks": "^7.2.0",
    "@storybook/jest": "^0.1.0",
    "@storybook/react": "^7.2.0",
    "@storybook/react-vite": "^7.2.0",
    "@storybook/test-runner": "^0.12.0",
    "@storybook/testing-library": "^0.2.0",
    "@types/jest": "^29.5.4",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@types/react-helmet": "^6.1.6",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "eslint-plugin-storybook": "^0.6.13",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "msw": "^1.2.2",
    "msw-storybook-addon": "^1.8.0",
    "prettier": "2.8.8",
    "prop-types": "^15.8.1",
    "sass": "^1.63.4",
    "storybook": "^7.2.0",
    "storybook-addon-react-router-v6": "^2.0.0",
    "stylelint": "^15.11.0",
    "stylelint-config-standard-scss": "^9.0.0",
    "stylelint-order": "^6.0.3",
    "svgo": "^3.0.2",
    "typescript": "^5.1.3",
    "vite": "4.3.9",
    "vite-plugin-svg-icons": "^2.0.1",
    "vitest": "^0.33.0"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
